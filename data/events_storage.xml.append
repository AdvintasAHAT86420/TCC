  <!--//////TRASH EATER DIALOGUE////////-->
  <event name="STORAGE_CHECK_CREW_TRASH">
    <text>What do you want to do with The Trash Eater?</text>
    <choice>
      <text>Nevermind, do something else.</text>
      <event load="STORAGE_CHECK"/>
    </choice>
    <choice>
